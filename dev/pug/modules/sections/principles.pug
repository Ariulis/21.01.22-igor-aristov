+b.SECTION#principles.principles.ibg
  img(src= content.principles.bgImage, alt="principles bg")
  +e.H2.title= content.principles.title
  +e.container
    +e.body
      each item, index in content.principles.content
        if index === 5
          +e.DIV(class=`item ${item.icon}`)
            span= content.principles.card
            | #{item.principle}
        else 
          +e.DIV(class=`item ${item.icon}`)= item.principle
